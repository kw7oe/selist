
<% user.subjects.each do |subject| %>
  <div class="col-3">
    <div class="box">
      <h2><%= subject.title %></h2>
      <% subject.lists.each do |list| %>
        <% list.incompleted_tasks(user).each do |task|%>
            <p id="<%= dom_id(task) %>">
              <%= check_box_tag "task_id",task.id,false,{:class=>"checkDone",:task=>task.id,:user=>user.id} %>
              <%= task.title %>
            </p>
        <% end %>
      <% end %> 
    </div> 
  </div>       
<% end %>