<div class="content">
  <div class = "container">
    <h1><%= subject_home %></h1>
  </div>
  <div class="container">
    <div class="flexbox">
          <% @subject.lists.each do |list| %>
            <div class="board box">
              <h3><%= list.title %></h3>
              <% list.incompleted_tasks(@student).each do |task|%>   
                <p id="<%= dom_id(task) %>">
                  <%= check_box_tag "task_id",task.id,false,{:class=>"checkDone",:task=>task.id,:user=>@student.id} %>
                  <%= task.title %>
                </p>
              <% end %>
            </div>
          <% end %> 
        </div>
    </div>
  </div>
</div>